{
  "name": "linklocal",
  "version": "2.6.1",
  "description": "Install local dependencies as symlinks.",
  "main": "index.js",
  "scripts": {
    "test": "nyc -s tape test/*.js | tap-spec && nyc report --reporter=text --reporter=html && standard"
  },
  "engines": {
    "npm": ">=2.0.0"
  },
  "author": "Tim Oxley <secoif@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "nyc": "^10.0.0",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "commander": "~2.9.0",
    "debug": "~2.2.0",
    "map-limit": "0.0.1",
    "mkdirp": "~0.5.1",
    "rimraf": "~2.5.4"
  },
  "bin": {
    "linklocal": "bin/linklocal.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timoxley/linklocal.git"
  },
  "keywords": [
    "npm",
    "packages",
    "symlink",
    "dependencies",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/timoxley/linklocal/issues"
  },
  "homepage": "https://github.com/timoxley/linklocal",
  "nyc": {
    "cache": true,
    "exclude": [
      "test/*.js",
      "node_modules"
    ]
  }
}
